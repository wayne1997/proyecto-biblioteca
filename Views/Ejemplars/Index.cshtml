@model IEnumerable<ProyectoIntegrador.Models.Ejemplar>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EstadoBibliografico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Disponibilidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.titulo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoBibliografico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Disponibilidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.titulo.Descripcion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EjemplarID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EjemplarID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EjemplarID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
